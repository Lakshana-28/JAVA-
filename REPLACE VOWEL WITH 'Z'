import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
       int i;

String s=sc.nextLine();
int l=s.length()-1;
//char v[]={'a','e','i','o','u'};
char c;
String a=" ";
char z='z';
for(i=0;i<=l;i++)
{
c=s.charAt(i);
if(c==97||c==101||c==105||c==111||c==117||c==65||c==69||c==73||c==79||c==85)
{
a=a+z;
}
else{
a=a+c;
}
}
        System.out.println(a);


}
}
